#Se importa la libreria para hacer la tabla
library(knitr)
library(kableExtra)
#Se crea una tabla para mostrar las variables y su descripción
tabla_variables <- data.frame(Variable = c("id", "area", "dificultad", "tiempo"),
Descripcion = c("Identificador único de cada participante", "Área tematica de la que se busca información", "Nivel de dificultad de los problemas de información", "Tiempo (en segundos) que se demora un participante en resolver el problema"))
#Se muestra la tabla
kable(tabla_variables, align = c("c", "l")) %>%
kable_styling(bootstrap_options = c("striped", "hover", "condensed", "responsive"),
full_width = FALSE,
position = "center") %>%
row_spec(0, bold = TRUE, color = "white", background = "black") %>%
column_spec(1, bold = TRUE, color = "blue")
#Se importa la libreria necesaria para filtrar por país
library(dplyr)
#Se importan las muestras
datos <- read.csv2("EP06 Datos.csv")
##Se filtran las muestras por arquitectura
datos_filtrados <- datos %>%
filter(area == "Arquitectura")
